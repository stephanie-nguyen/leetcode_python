def romanToInt(s):
  m = {
    'I': 1,
    'V': 5,
    'X': 10,
    'L': 50,
    'C': 100,
    'D': 500,
    'M': 1_000,
    'IV': 4,
    'IX': 9,
    'XL': 40,
    'XC': 90,
    'CD': 400,
    'CM': 900
  }
        
  i = 0
  res = 0
  while i<len(s):
    if i+1 < len(s) and s[i:i+2] in m:
      res+=m[s[i:i+2]]
      i+=2
    else:
      res+=m[s[i]]
      i+=1
        
  return res
  
s="IIII"
print(romanToInt(s))
